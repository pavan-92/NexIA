# Configurações do Frontend NexIA
# Crie um arquivo .env com base neste exemplo

# Firebase (necessário para autenticação)
VITE_FIREBASE_API_KEY=sua-chave-api-firebase
VITE_FIREBASE_APP_ID=seu-app-id-firebase
VITE_FIREBASE_PROJECT_ID=seu-projeto-firebase
VITE_FIREBASE_AUTH_DOMAIN=seu-dominio-firebase.firebaseapp.com
VITE_FIREBASE_STORAGE_BUCKET=seu-bucket-firebase.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=seu-messaging-sender-id

# Backend API URL
VITE_API_URL=http://localhost:5000/api

# Ambiente
VITE_NODE_ENV=development

# Configurações de interface
VITE_APP_NAME=NexIA
VITE_MAX_UPLOAD_SIZE=50