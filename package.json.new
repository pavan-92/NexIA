{
  "name": "nexia",
  "version": "1.0.0",
  "description": "NexIA - Plataforma de Prontuário Médico com IA",
  "main": "backend/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "ts-node-dev --respawn --transpile-only backend/index.ts",
    "dev:frontend": "cd frontend && vite",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && vite build",
    "build:backend": "tsc -p tsconfig.backend.json",
    "start": "node dist/backend/index.js",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "medical",
    "ai",
    "transcription",
    "prontuario",
    "healthcare"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@deepgram/sdk": "^3.1.7",
    "@neondatabase/serverless": "^0.9.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.29.4",
    "drizzle-zod": "^0.5.1",
    "express": "^4.19.1",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.18.0",
    "firebase-admin": "^12.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.2",
    "openai": "^4.28.4",
    "zod": "^3.22.4",
    "zod-validation-error": "^2.1.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-mysql-session": "^3.0.3",
    "@types/express-session": "^1.18.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.28",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.14",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  }
}